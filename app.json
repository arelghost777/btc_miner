{
  "expo": {
    "name": "ROBUST MINER",
    "slug": "btc_miner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/btcminer.png",
    "scheme": "btcminer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "enabled": false
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.btcminer.btcminer",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses Face ID to authenticate users"
      }
    },
    "android": {
      "package": "com.btcminer.btcminer",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/btcminer.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/btcminer.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraMavenRepos": [
              "https://maven.google.com"
            ]
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      ["expo-secure-store"]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4df4a74e-3898-420e-8e93-fa39494f95ea"
      }
    },
    "owner": "iamghost777"
  }
}
